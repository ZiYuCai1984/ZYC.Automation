<Project>

  <ItemGroup>
    <PackageReference Update="ZYC.CoreToolkit" Version="$(V_ZYC_CoreToolkit)" />
    <PackageReference Update="ZYC.CoreToolkit.Pro" Version="$(V_ZYC_CoreToolkit)" />
    <PackageReference Update="ZYC.CoreToolkit.Abstractions" Version="$(V_ZYC_CoreToolkit)" />
    <PackageReference Update="ZYC.CoreToolkit.Extensions.Autofac" Version="$(V_ZYC_CoreToolkit)" />
    <PackageReference Update="ZYC.CoreToolkit.Extensions.Settings" Version="$(V_ZYC_CoreToolkit)" />
    <PackageReference Update="ZYC.CoreToolkit.Extensions.WebView2" Version="$(V_ZYC_CoreToolkit)" />
  </ItemGroup>

  <!--!WARNING The value of $(NuGetPackageRoot) is inconsistent between the restore and build (it is empty during restore), $(NuGetPackageRoot) cannot be used here. !!-->
  <!--<Import Project="$(USERPROFILE)\.nuget\packages\ZYC.CoreToolkit\$(V_ZYC_CoreToolkit)\nuget.targets"></Import>-->

  <PropertyGroup>
    <V_Microsoft_Web_WebView2>1.0.3712-prerelease</V_Microsoft_Web_WebView2>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Update="Microsoft.Web.WebView2" Version="$(V_Microsoft_Web_WebView2)" />
  </ItemGroup>

  <PropertyGroup>
    <V_Autofac>9.0.0</V_Autofac>
    <V_Autofac_Extensions_DependencyInjection>10.0.0</V_Autofac_Extensions_DependencyInjection>
  </PropertyGroup>

  <ItemGroup>
    <!--!WARNING Autofac and Autofac.Extensions.DependencyInjection do not share the same version number !!-->
    <PackageReference Update="Autofac" Version="$(V_Autofac)" />
    <PackageReference Update="Autofac.Extensions.DependencyInjection"
                      Version="$(V_Autofac_Extensions_DependencyInjection)" />
  </ItemGroup>

  <PropertyGroup>
    <V_Microsoft_Extensions_Logging_Abstractions>10.0.2</V_Microsoft_Extensions_Logging_Abstractions>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Update="Microsoft.Extensions.Logging" 
                      Version="$(V_Microsoft_Extensions_Logging_Abstractions)" />
    <PackageReference Update="Microsoft.Extensions.Logging.Abstractions" 
                      Version="$(V_Microsoft_Extensions_Logging_Abstractions)" />
    <PackageReference Update="Microsoft.Extensions.Hosting.Abstractions" 
                      Version="$(V_Microsoft_Extensions_Logging_Abstractions)" />
  </ItemGroup>

  <PropertyGroup>
    <V_MahApps_Metro>3.0.0-rc0529</V_MahApps_Metro>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Update="MahApps.Metro" Version="$(V_MahApps_Metro)" />
  </ItemGroup>

  <PropertyGroup>
    <V_MahApps_Metro_IconPacks_Material>6.2.1</V_MahApps_Metro_IconPacks_Material>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Update="MahApps.Metro.IconPacks.Material" Version="$(V_MahApps_Metro_IconPacks_Material)" />
  </ItemGroup>

  <PropertyGroup>
    <V_AvalonEdit>6.3.1.120</V_AvalonEdit>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Update="AvalonEdit" Version="$(V_AvalonEdit)" />
  </ItemGroup>

