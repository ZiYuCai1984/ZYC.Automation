    <PackageReference Update="Emoji.Wpf" Version="$(V_Emoji_Wpf)" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="Aspire.Hosting.AppHost" Version="$(V_Aspire)" />
  </ItemGroup>


  <PropertyGroup>
    <V_NuGet>7.0.1</V_NuGet>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Update="NuGet.Frameworks" Version="$(V_NuGet)" />
    <PackageReference Update="NuGet.Protocol" Version="$(V_NuGet)" />
  </ItemGroup>

  <PropertyGroup>
    <V_Newtonsoft_Json>13.0.3</V_Newtonsoft_Json>
  </PropertyGroup>

  <!--!WARNING In order to be compatible with Newtonsoft.Json version in NuGet.Protocol and TickerQ-->
  <ItemGroup>
    <PackageReference Update="Newtonsoft.Json" Version="$(V_Newtonsoft_Json)" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="WPFHexaEditor" Version="2.1.7" />
